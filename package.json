{
  "name": "stack-workflow",
  "version": "0.1.0",
  "description": "Stack Workflow",
  "main": "index.js",
  "scripts": {
    "tiye": "rsync -r dist/ repo.tiye.me:repo/mvc-works/stack-workflow/",
    "cljs-release": "shadow-cljs release release",
    "watch": "shadow-cljs watch dev",
    "compile": "shadow-cljs compile npm",
    "cljs": "shadow-cljs watch npm",
    "html-dev": "env=dev node target/app.render.js",
    "html": "node target/app.render.js",
    "hmr": "webpack-dev-server --config webpack.dev.coffee --hot-only",
    "webpack": "webpack --config webpack.release.coffee --hide-modules --progress",
    "dev": "yarn compile && yarn html-dev && yarn hmr",
    "del": "rm -rf dist/*",
    "serve": "http-server dist",
    "build": "yarn del && yarn cljs-release && yarn webpack && yarn compile && yarn html && yarn serve"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/stack-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "coffee-script": "^1.12.6",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "http-server": "^0.10.0",
    "respo-ui": "^0.1.2",
    "shadow-cljs": "^0.12.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
