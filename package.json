{
  "name": "stack-workflow",
  "version": "0.1.0",
  "description": "Stack Workflow",
  "main": "index.js",
  "scripts": {
    "cljs-once": "shadow-cljs --once",
    "cljs": "shadow-cljs --dev",
    "html-dev": "env=dev node ./entry/html.js",
    "html": "node ./entry/html.js",
    "hmr": "webpack-dev-server --config webpack.dev.coffee --hot-only",
    "tiye": "bash entry/rsync.sh",
    "webpack": "webpack",
    "dev": "yarn html-dev && yarn hmr",
    "build": "yarn cljs-once && yarn webpack && yarn html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/stack-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "shadow-cljs": {
    "source-paths": [
      "src",
      "polyfill"
    ],
    "output-dir": "compiled",
    "dependencies": [
      [
        "mvc-works/hsl",
        "0.1.2"
      ],
      [
        "respo/ui",
        "0.1.9"
      ],
      [
        "respo",
        "0.4.2"
      ]
    ]
  },
  "devDependencies": {
    "coffee-script": "^1.12.6",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.0",
    "respo-ui": "^0.1.2",
    "shadow-cljs": "^0.5.1",
    "style-loader": "^0.16.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
